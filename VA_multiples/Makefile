SRC = ./src/
R = ./results/
#******************************************************************************
############ 	Run python script
#******************************************************************************
all: analize clean

analize:
	python3 $(SRC)main.py

#******************************************************************************
############ 	CLEAN FOR ALL
#******************************************************************************


.PHONY: clean
clean:
	rm -rf $(R)*.png
